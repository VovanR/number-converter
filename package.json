{
  "name": "number-converter",
  "version": "0.0.1",
  "description": "Convert decimal and binary numbers",
  "private": true,
  "license": "MIT",
  "repository": "VovanR/number-converter",
  "author": "Vladimir Rodkin <mail@vovanr.com> (https://github.com/VovanR)",
  "engines": {
    "node": ">=9"
  },
  "scripts": {
    "test": "npm run lint && npm run test:testcafe",
    "lint": "xo --plugin=testcafe --extend=\"plugin:testcafe/recommended\"",
    "test:testcafe": "testcafe \"firefox:headless\" test-cafe/test.js --app \"http-server . -s\"",
    "start": "http-server .",
    "prepush": "npm test",
    "precommit": "npm test",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major"
  },
  "keywords": [],
  "dependencies": {},
  "devDependencies": {
    "conventional-changelog-cli": "^1.3.5",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "mversion": "^1.10.1",
    "testcafe": "^0.18.6",
    "xo": "^0.18.2"
  },
  "peerDependencies": {},
  "bugs": {
    "url": "https://github.com/VovanR/number-converter/issues"
  },
  "homepage": "https://github.com/VovanR/number-converter#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
